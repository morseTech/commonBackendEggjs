{
  "name": "cb-egg",
  "version": "1.0.5",
  "description": "cb-egg 基于eggjs的通用后端开发框架 common-backend-eggjs",
  "private": false,
  "main": "index.js",
  "files": [
    "app",
    "config",
    "app.js",
    "agent.js",
    "lib",
    "libs"
  ],
  "keywords": [
    "egg",
    "framework",
    "egg-framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morseTech/commonBackendEggjs.git"
  },
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "egg": "^3",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^2",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "mysql2": "^3.2.4",
    "openai": "^4.33.0",
    "redis": "^4.7.0",
    "stream-wormhole": "^1.1.0"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "egg-bin dev --env=local",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test --env=unittest",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "db2model": "cd libs/sequelize-db && node gen-models.js",
    "model2db": "cd libs/sequelize-db && node gen-database.js"
  },
  "author": "hongfu_zhang@163.com",
  "license": "MIT"
}
